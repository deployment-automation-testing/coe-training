        <definition>load(
  local!users:getdistinctusers(#&quot;_a-0000db88-8ba1-8000-c510-010000010000_2534&quot;),
  local!indices:wherecontains(ri!zooId,apply(#&quot;_a-0000db88-8ba1-8000-c510-010000010000_2310&quot;,local!users)),
