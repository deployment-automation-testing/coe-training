    <rule>
        <name>AM_getIconForApproval</name>
        <uuid>_a-0000db88-8ba1-8000-c510-010000010000_2622</uuid>
        <description>Returns icon for approval boolean</description>
        <parentUuid>_a-0000db88-8ba1-8000-c510-010000010000_2228</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(isnull(ri!value),&#xD;
&#xD;
if(status &lt;= 2, #&quot;SYSTEM_SYSRULES_documentImage&quot;(document: #&quot;SYSTEM_SYSRULES_iconIndicator&quot;(icon: &quot;WAIT_CLOCK&quot;), caption:#&quot;_a-0000db88-8ba1-8000-c510-010000010000_2550&quot;[4]),&#xD;
#&quot;SYSTEM_SYSRULES_documentImage&quot;(document: #&quot;SYSTEM_SYSRULES_iconIndicator&quot;(icon: &quot;TREND_FLAT&quot;), caption:#&quot;_a-0000db88-8ba1-8000-c510-010000010000_2550&quot;[3])),&#xD;
&#xD;
choose(&#xD;
  1 + ri!value,&#xD;
  #&quot;SYSTEM_SYSRULES_documentImage&quot;(document: #&quot;SYSTEM_SYSRULES_iconIndicator&quot;(icon: &quot;STATUS_ERROR&quot;), caption:#&quot;_a-0000db88-8ba1-8000-c510-010000010000_2550&quot;[1]),&#xD;
  #&quot;SYSTEM_SYSRULES_documentImage&quot;(document: #&quot;SYSTEM_SYSRULES_iconIndicator&quot;(icon: &quot;STATUS_OK&quot;), caption: #&quot;_a-0000db88-8ba1-8000-c510-010000010000_2550&quot;[2])&#xD;
)&#xD;
)</definition>
        <namedTypedValue>
            <name>value</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>status</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
